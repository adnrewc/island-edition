{% set links = collections.all | eleventyNavigation %}
<nav aria-label="Primary">
  <div class="nav__inner">
    <a class="nav__brand" href="/">{{ metadata.site_title }}</a>
    <button class="nav__toggle" type="button" aria-expanded="false" aria-controls="primary-nav">Menu</button>
    <ul id="primary-nav" class="nav__list">
      {% for link in links %}
        <li class="nav__item">
          <a href="{{ link.url }}" {% if link.url == page.url %}aria-current="page"{% endif %}>{{ link.title }}</a>
        </li>
      {% endfor %}
    </ul>
  </div>
</nav>
